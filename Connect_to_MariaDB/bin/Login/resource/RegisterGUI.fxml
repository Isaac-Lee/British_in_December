<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="250.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Login.java.Register.RegisterController">
   <children>
      <Label layoutX="126.0" layoutY="14.0" text="회원가입" />
      <HBox layoutX="73.0" layoutY="205.0" spacing="50.0">
         <children>
            <Button fx:id="RegisterBotton" mnemonicParsing="false" onAction="#register" text="회원가입" />
            <Button fx:id="CancleBotton" mnemonicParsing="false" onAction="#cancle" text="취소" />
         </children>
      </HBox>
      <VBox layoutX="35.0" layoutY="72.0" spacing="20.0">
         <children>
            <HBox spacing="30.0">
               <children>
                  <Label text="아이디" />
                  <TextField fx:id="IdTextField" />
               </children>
            </HBox>
            <HBox spacing="20.0">
               <children>
                  <Label text="비밀번호" />
                  <PasswordField fx:id="PassTextField" />
               </children>
            </HBox>
            <HBox spacing="32.0">
               <children>
                  <Label text="재입력" />
                  <PasswordField fx:id="PassVerifyTextField" />
               </children>
            </HBox>
         </children>
      </VBox>
      <Label fx:id="ErrorMessageLabel" alignment="CENTER" layoutX="4.0" layoutY="40.0" prefHeight="14.0" prefWidth="293.0" />
   </children>
</AnchorPane>
