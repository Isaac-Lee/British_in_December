<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="500.0" prefWidth="400.0"
	xmlns="http://javafx.com/javafx/11.0.1"
	xmlns:fx="http://javafx.com/fxml/1" fx:controller="Manager.Java.Controllers.ScooterInfoGuiController">
	<children>
		<Button fx:id="backButton" layoutX="344.0" layoutY="14.0"
			mnemonicParsing="false" onAction="#backButtonHandler" text="Back" />
		<Label layoutX="170.0" layoutY="57.0" text="ScooterInfo" />
		<VBox layoutX="87.0" layoutY="120.0" prefHeight="100.0"
			prefWidth="226.0">
			<children>
				<Label alignment="CENTER" maxHeight="1.7976931348623157E308"
					maxWidth="1.7976931348623157E308" text="스쿠터 목록 확인"
					VBox.vgrow="ALWAYS" />
				<Button fx:id="confirmScooterListButton" alignment="CENTER"
					maxHeight="1.7976931348623157E308"
					maxWidth="1.7976931348623157E308" mnemonicParsing="false"
					onAction="#confirmScooterListButtonHandler" text="확인하기"
					VBox.vgrow="ALWAYS">
					<VBox.margin>
						<Insets bottom="20.0" left="50.0" right="50.0" top="20.0" />
					</VBox.margin>
				</Button>
			</children>
		</VBox>
		<VBox layoutX="87.0" layoutY="240.0" prefHeight="100.0"
			prefWidth="226.0">
			<children>
				<Label alignment="CENTER" maxHeight="1.7976931348623157E308"
					maxWidth="1.7976931348623157E308" text="스쿠터 정보 확인"
					VBox.vgrow="ALWAYS" />
				<Button fx:id="confirmScooterInformationButton"
					alignment="CENTER" maxHeight="1.7976931348623157E308"
					maxWidth="1.7976931348623157E308" mnemonicParsing="false"
					onAction="#confirmScooterInformationButtonHandler" text="확인하기"
					VBox.vgrow="ALWAYS">
					<VBox.margin>
						<Insets bottom="20.0" left="50.0" right="50.0" top="20.0" />
					</VBox.margin>
				</Button>
			</children>
		</VBox>
		<VBox layoutX="87.0" layoutY="360.0" prefHeight="100.0"
			prefWidth="226.0">
			<children>
				<Label alignment="CENTER" maxHeight="1.7976931348623157E308"
					maxWidth="1.7976931348623157E308" text="스쿠터 위치 조회"
					VBox.vgrow="ALWAYS" />
				<Button fx:id="findScooterLocationButton" alignment="CENTER"
					maxHeight="1.7976931348623157E308"
					maxWidth="1.7976931348623157E308" mnemonicParsing="false"
					onAction="#findScooterLocationButtonHandler" text="조회하기"
					VBox.vgrow="ALWAYS">
					<VBox.margin>
						<Insets bottom="20.0" left="50.0" right="50.0" top="20.0" />
					</VBox.margin>
				</Button>
			</children>
		</VBox>
	</children>
</AnchorPane>
