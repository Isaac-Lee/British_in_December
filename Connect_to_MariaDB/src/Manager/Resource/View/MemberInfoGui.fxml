<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="400.0"
	xmlns="http://javafx.com/javafx/11.0.1"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="Manager.Java.Controllers.MemberInfoGuiController">
	<children>
		<ListView fx:id="memberListView" layoutX="14.0"
			layoutY="86.0" prefHeight="366.0" prefWidth="250.0" />
		<Button fx:id="deleteButton" layoutX="300.0" layoutY="166.0"
			mnemonicParsing="false" onAction="#deleteButtonHandler"
			prefHeight="40.0" text="삭제하기" />
		<Label layoutX="14.0" layoutY="63.0" text="멤버 목록" />
		<Label layoutX="144.0" layoutY="29.0" text="MemberManagement" />
		<Button fx:id="backButton" layoutX="344.0" layoutY="14.0"
			mnemonicParsing="false" onAction="#backButtonHandler" text="Back" />
		<Label layoutX="14.0" layoutY="467.0" text="ID : " />
		<TextField fx:id="memberIDTextField" layoutX="69.0"
			layoutY="463.0" prefHeight="22.0" prefWidth="195.0"
			promptText="회원의 ID를 입력하세요" />
		<Button fx:id="findIDButton" layoutX="300.0" layoutY="463.0"
			mnemonicParsing="false" onAction="#findIDButtonHandler" text="ID 찾기" />
		<Button fx:id="checkInfoButton" layoutX="300.0" layoutY="230.0"
			mnemonicParsing="false" onAction="#checkInfoButtonHandler"
			prefHeight="40.0" prefWidth="64.0" text="조회하기" />
	</children>
</AnchorPane>
